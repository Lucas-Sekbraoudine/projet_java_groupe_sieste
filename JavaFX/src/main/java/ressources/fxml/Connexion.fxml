<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<?import javafx.scene.control.PasswordField?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ConnexionController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Text layoutX="256.0" layoutY="63.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Connexion">
                    <font>
                        <Font size="27.0" />
                    </font>
                </Text>
                <TextField id="userName" fx:id="userName" layoutX="182.0" layoutY="124.0" prefHeight="25.0" prefWidth="276.0"/>
                <Text layoutX="65.0" layoutY="144.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login : ">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
                <Text layoutX="28.0" layoutY="208.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mot de passe : ">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
                <PasswordField id="passWord" fx:id="passWord" layoutX="182.0" layoutY="188.0" prefHeight="25.0" prefWidth="276.0"/>
                <Button layoutX="257.0" layoutY="252.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="127.0" text="Connexion" onAction="#handleConnexionPress">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Text layoutX="234.0" layoutY="356.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Pas de compte ?" />
                <Hyperlink id="inscription" fx:id="inscription" layoutX="335.0" layoutY="340.0" text="Inscription" onAction="#handleInscriptionPress"/>
            </children>
        </AnchorPane>
    </children>
</VBox>