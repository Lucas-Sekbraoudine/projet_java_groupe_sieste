<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.PasswordField?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/13" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.InscriptionController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <children>
                <Text layoutX="255.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Inscription">
                    <font>
                        <Font size="27.0" />
                    </font>
                </Text>
                <TextField layoutX="180.0" layoutY="174.0" prefHeight="25.0" prefWidth="276.0" id="userName" fx:id="userName"/>
                <Text id="isUserExist" fx:id="isUserExist" layoutX="470.0" layoutY="190.0" strokeType="OUTSIDE" strokeWidth="0.0" text="L'utilisateur existe déjà ! " visible="false">
                    <font>
                        <Font size="12.0" />
                    </font>
                </Text>
                <Text layoutX="65.0" layoutY="195.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Login : ">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
                <Text layoutX="28.0" layoutY="237.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Mot de passe : ">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
                <PasswordField layoutX="180.0" layoutY="216.0" prefHeight="25.0" prefWidth="276.0" id="passWord" fx:id="passWord"/>
                <Button layoutX="258.0" layoutY="271.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="127.0" text="Inscription" id="inscription" fx:id="inscription" onAction="#handleInscriptionPress">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Text layoutX="250.0" layoutY="367.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Déjà inscrit ?" />
                <Hyperlink id="connexion" fx:id="connexion" layoutX="321.0" layoutY="351.0" text="Connexion" onAction="#handleConnexionPress"/>
                <TextField layoutX="180.0" layoutY="134.0" prefHeight="25.0" prefWidth="276.0" id="lastName" fx:id="lastName"/>
                <Text layoutX="67.0" layoutY="155.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Nom : ">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
                <TextField layoutX="180.0" layoutY="95.0" prefHeight="25.0" prefWidth="276.0"  id="firstName" fx:id="firstName"/>
                <Text layoutX="54.0" layoutY="115.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Prénom : ">
                    <font>
                        <Font size="21.0" />
                    </font>
                </Text>
            </children>
        </AnchorPane>
    </children>
</VBox>